{
  "name": "md2gslides",
  "version": "0.4.0",
  "description": "Convert Markdown to Google Slides",
  "main": "index.js",
  "files": [
    "index.js",
    "bin/",
    "lib/",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "babel --source-maps both -d lib/ src/",
    "prepublish": "npm run compile",
    "exec": "npm run compile && node bin/md2gslides.js",
    "test": "npm run compile && mocha --require @babel/register --timeout 5000",
    "lint": "./node_modules/.bin/eslint --fix src bin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//googlesamples/md2googleslides"
  },
  "keywords": [
    "markdown",
    "google",
    "slides"
  ],
  "author": "Steven Bazyl",
  "license": "Apache-2.0",
  "dependencies": {
    "argparse": "^1.0.7",
    "babel-polyfill": "^6.26.0",
    "debug": "^4.1.1",
    "extend": "^3.0.0",
    "file-url": "^2.0.2",
    "google-auth-library": "^2.0.2",
    "googleapis": "^36.0.0",
    "highlight.js": "^9.15.6",
    "inline-styles-parse": "^1.2.0",
    "jsonfile": "^5.0.0",
    "layout": "^2.2.0",
    "lodash": "^4.17.11",
    "lowdb": "^1.0",
    "lowlight": "^1.12.0",
    "markdown": "^0.5.0",
    "markdown-it": "^8.0.0",
    "markdown-it-attrs": "^2.3.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.2.0",
    "markdown-it-expand-tabs": "^1.0.11",
    "markdown-it-fence": "^0.1.3",
    "markdown-it-lazy-headers": "^0.1.3",
    "markdown-it-video": "^0.6.3",
    "mathjax-node": "^2.1.1",
    "md5-file": "^4.0.0",
    "mime-types": "^2.1.22",
    "mkdirp": "^0.5.1",
    "native-css": "^2.0.0",
    "node-fetch": "^2.3.0",
    "opener": "^1.4.2",
    "parse-color": "^1.0.0",
    "parse5": "^5.1.0",
    "probe-image-size": "^4.0.0",
    "promise": "^8.0.3",
    "promise-retry": "^1.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sharp": "^0.21.3",
    "tmp": "0.0.33",
    "tmp-promise": "^1.0.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.3.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.8.0",
    "nock": "^10.0.6"
  },
  "bin": {
    "md2gslides": "bin/md2gslides.js"
  }
}
